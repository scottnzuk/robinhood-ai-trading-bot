{
  "compress": true,
  "includePatterns": [
    "**/*.py",
    "**/*.md",
    "**/*.json",
    "**/*.yaml",
    "**/*.yml"
  ],
  "ignorePatterns": [
    "**/*.log",
    "**/node_modules/**",
    "**/__pycache__/**",
    "**/.git/**",
    "**/venv/**",
    "**/env/**",
    "**/dist/**",
    "**/build/**"
  ]
}